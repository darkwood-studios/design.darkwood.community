		<type>
			<name>design.darkwood.community.recentActivityEvent.topic</name>
			<definitionname>com.woltlab.wcf.user.recentActivityEvent</definitionname>
			<classname>community\system\user\activity\event\TopicUserActivityEvent</classname>
		</type>
		<type>

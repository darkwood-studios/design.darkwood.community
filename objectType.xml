			<classname>wcf\system\user\notification\object\type\LikeUserNotificationObjectType</classname>
			<category>design.darkwood.community</category>
		</type>

        <!-- comments -->
		<type>
			<name>design.darkwood.community.topicComment</name>
			<definitionname>com.woltlab.wcf.comment.commentableContent</definitionname>
			<classname>community\system\comment\manager\TopicCommentManager</classname>
		</type>

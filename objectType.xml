		<!-- /Category -->
		
		<!-- ACL -->
		<type>
			<name>design.darkwood.community.topic.category</name>
			<definitionname>com.woltlab.wcf.acl</definitionname>
		</type>
		<!-- /ACL -->

        <!-- message -->
		<type>
			<name>design.darkwood.community.topic</name>
			<definitionname>com.woltlab.wcf.message</definitionname>
		</type>

